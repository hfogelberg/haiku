{
  "name": "haiku",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Henrik Fogelberg <henrik.fogelberg@gmail.com>",
  "private": true,
  "scripts": {
    "dev-app": "npm run haiku_server | npm run haiku",
    "server": "nodemon index.js",
    "haiku_server": "pm2 start index.js --watch --name 'haiku_server' --trace",
    "haiku": "cross-env NODE_ENV=development pm2 start webpack-dev-server  --name haiku -- --open --hot",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cloudinary": "^1.8.0",
    "connect-multiparty": "^2.0.0",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.1",
    "validator": "^7.0.0",
    "vue": "^2.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "concurrently": "^3.4.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.5.0",
    "pm2": "^2.4.2",
    "sass-loader": "^5.0.1",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
